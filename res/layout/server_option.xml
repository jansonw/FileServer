<?xml version="1.0" encoding="utf-8"?>
    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"        android:id="@+id/relativeLayout1"        android:layout_width="match_parent"        android:layout_height="match_parent"        android:orientation="vertical" >        <LinearLayout            android:id="@+id/linearLayout1"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentTop="true" >            <TextView                android:id="@+id/options_file"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginLeft="26dp"                android:text="Large Text"                android:textAppearance="?android:attr/textAppearanceLarge" />        </LinearLayout>        <ImageButton            android:id="@+id/server_downloadBtn"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentLeft="true"            android:layout_below="@+id/linearLayout1"            android:layout_marginLeft="20dp"            android:layout_marginTop="18dp"            android:src="@drawable/download" />        <ImageButton            android:id="@+id/server_propertiesBtn"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignLeft="@+id/server_downloadBtn"            android:layout_below="@+id/server_downloadBtn"            android:src="@android:drawable/ic_menu_manage" />        <ImageButton            android:id="@+id/server_deleteBtn"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignLeft="@+id/server_propertiesBtn"            android:layout_below="@+id/server_propertiesBtn"            android:src="@android:drawable/ic_menu_delete" />        <TextView            android:id="@+id/textView3"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignBottom="@+id/server_deleteBtn"            android:layout_marginBottom="16dp"            android:layout_marginLeft="24dp"            android:layout_toRightOf="@+id/server_deleteBtn"            android:text="Delete"            android:textAppearance="?android:attr/textAppearanceLarge" />        <TextView            android:id="@+id/textView2"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignBottom="@+id/server_propertiesBtn"            android:layout_alignLeft="@+id/textView3"            android:layout_marginBottom="18dp"            android:text="Properties"            android:textAppearance="?android:attr/textAppearanceLarge" />
        <TextView            android:id="@+id/server_download"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignBottom="@+id/server_downloadBtn"            android:layout_alignLeft="@+id/textView2"            android:layout_marginBottom="19dp"            android:text="Download"            android:textAppearance="?android:attr/textAppearanceLarge" />    </RelativeLayout>